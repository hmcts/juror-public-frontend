{% extends "./template.njk" %}


{% block head %}
  <!--[if !IE 8]><!-->
    <link href="../../../css/govuk-frontend.css" rel="stylesheet" />
    <!--<link href="../../css/juror-digital.css" rel="stylesheet" />-->
    <!--<link href="../../css/style.css" rel="stylesheet" />-->
    <link href="../../../css/jd-style.css" rel="stylesheet" />
  <!--<![endif]-->

  <script type="text/javascript" src="{{ assetPath }}js/html5shiv.min.js"></script>
  <script type="text/javascript" src="{{ assetPath }}js/respond.min.js"></script>

  {# For Internet Explorer 8, you need to compile specific stylesheet #}
  {# see https://github.com/alphagov/govuk-frontend/blob/master/docs/installation/supporting-internet-explorer-8.md #}
