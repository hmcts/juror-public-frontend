{% block top_of_page %}{% endblock %}
<!DOCTYPE html>
<!--[if lt IE 9]><html class="lte-ie8" lang="{{ html_lang|default('en') }}"><![endif]-->
<html lang="{{ texts.INTERFACE.HTML_LANG|default('en') }}">
  <head>
    <meta charset="utf-8" />
    <title>
      {% block page_title %}
      {{ texts.INTERFACE.SERVICE_TITLE }} - {{ texts.INTERFACE.GOV }}
      {% endblock %}
    </title>

    {% if trackingCode %}
    <!-- Google Analytics -->
    <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('set', 'anonymizeIp', true);
    ga('create', '{{ trackingCode }}', 'auto');
    ga('send', 'pageview');
    </script>
    <!-- End Google Analytics -->
    {% endif %}

    <!--[if gt IE 8]><!--><link href="{{ assetPath }}css/govuk-template.css?0.17.3" media="screen" rel="stylesheet" /><!--<![endif]-->
    <!--[if IE 6]><link href="{{ assetPath }}css/govuk-template-ie6.css?0.17.3" media="screen" rel="stylesheet" /><![endif]-->
    <!--[if IE 7]><link href="{{ assetPath }}css/govuk-template-ie7.css?0.17.3" media="screen" rel="stylesheet" /><![endif]-->
    <!--[if IE 8]><link href="{{ assetPath }}css/govuk-template-ie8.css?0.17.3" media="screen" rel="stylesheet" /><![endif]-->
    <link href="{{ assetPath }}css/govuk-template-print.css?0.17.3" media="print" rel="stylesheet" />

    <!--[if IE 8]><link href="{{ assetPath }}css/fonts-ie8.css?0.17.3" media="all" rel="stylesheet" /><![endif]-->
    <!--[if gte IE 9]><!--><link href="{{ assetPath }}css/fonts.css?0.17.3" media="all" rel="stylesheet" /><!--<![endif]-->
    <!--[if lt IE 9]><script src="{{ assetPath }}js/ie.js?0.17.3"></script><![endif]-->

    <link rel="shortcut icon" href="{{ assetPath }}img/favicon.ico?0.17.3" type="image/x-icon" />
    <link rel="mask-icon" href="{{ assetPath }}img/gov.uk_logotype_crown.svg?0.17.3" color="#0b0c0c">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ assetPath }}img/apple-touch-icon-152x152.png?0.17.3">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ assetPath }}img/apple-touch-icon-120x120.png?0.17.3">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ assetPath }}img/apple-touch-icon-76x76.png?0.17.3">
    <link rel="apple-touch-icon" href="{{ assetPath }}img/apple-touch-icon-60x60.png?0.17.3">

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:image" content="{{ assetPath }}img/opengraph-image.png?0.17.3">
    <meta name="pageIdentifier" content="{% block page_identifier %}{% endblock %}">
    <meta name="robots" content="noindex, nofollow">

    {% block head %}{% endblock %}
  </head>

  <body class="{% block body_classes %}{% endblock %}">
    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

    {% block body_start %}{% endblock %}

    <div id="skiplink-container">
      <div>
        <a href="#content" class="skiplink">{{texts.INTERFACE.SKIP_TO_MAIN_CONTENT}}</a>
      </div>
    </div>


    <header id="global-header" class="{% block header_class %}{% endblock %}">
      <div class="header-wrapper">
        <div class="header-global">
          <div class="header-logo">
            <a target="_blank" href="{{ homepage_url|default('https://www.gov.uk') }}" title="{{ logo_link_title|default('Go to the GOV.UK homepage') }}" id="logo" class="content" alt="{{ texts.INTERFACE.LINK_OPENS_NEW_WINDOW }}">
              <img src="{{ assetPath }}img/gov.uk_logotype_crown_invert_trans.png?0.18.2" width="35" height="31" alt="{{texts.INTERFACE.GOV}} logo"> {{ global_header_text|default('GOV.UK') }}
            </a>
          </div>
          {% block inside_header %}{% endblock %}
        </div>
        {% block proposition_header %}{% endblock %}
      </div>
    </header>


    {% block after_header %}{% endblock %}

    {% block nav_header %}{% endblock %}

    {% if cookieText %}
    <div id="global-cookie-message">
      {% block cookie_message %}{% endblock %}
    </div>
    {% endif %}

    <div id="global-header-bar"></div>

    <main id="content" class="main-content" tabindex="-1">
      {% include "includes/phase_banner_beta.njk" %}
      {% block content %}{% endblock %}
    </main>

    <footer class="group js-footer" id="footer">

      <div class="footer-wrapper">
        {% block footer_top %}{% endblock %}

        <div class="footer-meta">

          <div class="footer-meta-inner">
            {% block footer_support_links %}{% endblock %}

            <div class="open-government-licence">
              <p class="logo"><a target="_blank" href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license" alt="{{ texts.INTERFACE.LINK_OPENS_NEW_WINDOW }}">{{ texts.INTERFACE.OPEN_GOV_LICENCE }}</a></p>

              {% block licence_message %}
              <p>
                {{ texts.INTERFACE.CONTENT_AVAILABLE_UNDER }} <a target="_blank" href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license" alt="{{ texts.INTERFACE.LINK_OPENS_NEW_WINDOW }}">{{ texts.INTERFACE.OPEN_GOV_LICENCE }} {{ texts.INTERFACE.OPEN_GOV_LICENCE_V }}</a>,
                {{ texts.INTERFACE.EXCEPT_OTHERWISE }}
              </p>
              {% endblock %}

            </div>
          </div>

          <div class="copyright">
            <a target="_blank" href="http://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/" alt="{{ texts.INTERFACE.LINK_OPENS_NEW_WINDOW }}">{{ crown_copyright_message|default('&copy; Crown copyright')|safe }}</a>
          </div>
        </div>
      </div>
    </footer>

    <div id="global-app-error" class="app-error hidden"></div>

    {% block body_end %}{% endblock %}
  </body>
</html>
