{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "env": {
    "es6": true,
    "node": true,
    "protractor": true
  },
  "globals": {
    "browser": true,
    "element": true,
    "by": true,
    "expect": true
  },
  "rules": {
    "max-len": 0,
    "class-methods-use-this": 0
  }
}
